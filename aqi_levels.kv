<LayoutAqiMenu@ScrollView>:
    do_scroll_x: False
    LayoutAqiMenuChild:

<LayoutAqiMenuChild@BoxLayout>:
    size_hint_min_y: '6500dp'
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size

    orientation: 'vertical'
    spacing: '15dp'
    padding: [0, '15dp', 0, '15dp']
    
    Button:
        text: "[b]NEAREST AVAILABLE STATION[/b]"
        size_hint: 0.8, 0.7
        #height = '100dp'
        pos_hint: {'center_x':0.5}
        background_normal: ''
        background_color: 48/255, 194/255, 141/255, 1
        font_size: '15dp'
        markup: True
        on_press:
            app.get_default_aqi_info()
            app.manager.push('AqiInfoScreen')
    
