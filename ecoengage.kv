<ScrollViewExample@ScrollView>:
    do_scroll_x: False
    MainLayout:
        size_hint: 1, None
        height: self.minimum_height

<MainLayout@BoxLayout>:
    canvas.before:
        Rectangle:
            size: self.size
            source: 'images/home_bg_2.jpg'

    orientation: 'vertical'
    spacing: '15dp'
    padding: [0, 0, 0, '15dp']
    Label:
        text: 'ECO ENGAGE'
        font_name: 'fonts/NatureBeautyPersonalUse-9Y2DK.ttf'
        font_size: '30dp'
        size_hint: 1, 1
        height: '100dp'
        color: 0, 74/255, 172/255, 1
        #48/255, 194/255, 141/255, 1
    
    Button:
        text: 'Quiz'
        size_hint: 0.9, 1
        height: '100dp'
        pos_hint: {'center_x':0.5}
        background_normal: ''
        background_color: 48/255, 194/255, 141/255, 1
        font_size: '25dp'
        on_press: app.manager.push('QuizScreen')

    Button:
        text: 'AQI LEVELS'
        size_hint: 0.9, 1
        height: '100dp'
        pos_hint: {'center_x':0.5}
        background_normal: ''
        background_color: 48/255, 194/255, 141/255, 1
        font_size: '25dp'
        on_release: app.manager.push('AqiScreen')    
     
    Button:
        text: 'Study Material'
        size_hint: 0.9, 1
        height: '100dp'
        pos_hint: {'center_x':0.5}
        background_normal: ''
        background_color: 48/255, 194/255, 141/255, 1
        font_size: '25dp'
        on_press: app.manager.push('StudyMaterialScreen')
    
    Button:
        text: 'Government Schemes'
        size_hint: 0.9, 1
        height: '100dp'
        pos_hint: {'center_x':0.5}
        background_normal: ''
        background_color: 48/255, 194/255, 141/255, 1
        font_size: '25dp'
        on_press: app.manager.push('GovSchemesScreen')

    Button:
        text: 'About'
        size_hint: 0.9, 1
        height: '75'
        pos_hint: {'center_x':0.5}
        background_normal: ''
        background_color: 48/255, 194/255, 141/255, 1
        font_size: '22dp'
        on_release: app.manager.push('AboutAppScreen')
