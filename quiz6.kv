<LayoutQuiz6Correct1@BoxLayout>:
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size
    orientation: 'vertical'
    BackgroundLabel:
        text: '[b]CORRECT![/b]'
