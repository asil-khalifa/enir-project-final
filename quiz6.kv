#:import BackgroundLabel_def BackgroundLabel_def


<LayoutQuiz6Question1@BoxLayout>:
    
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size
    orientation: 'vertical'
    spacing: '15dp'
    padding: '10dp'

    BackgroundLabel:
        text: "Which of the following is not a fossil fuel?"
        size_hint: 1, 1
        background_color: 48/255, 194/255, 141/255, 1
        text_size: root.width/1.1, None
        size: self.texture_size
        markup: True

    Button:
        text: 'Natural Gas'
        size_hint: 0.8, 0.7
        pos_hint: {'center_x':0.5}
        background_normal: ''
        background_color: 48/255, 194/255, 141/255, 1
        text_size: root.width/1.5, None
        on_release: app.manager.special_push('Quiz6Wrong1')
    
    Button:
        text: 'Petrol'
        size_hint: 0.8, 0.7
        pos_hint: {'center_x':0.5}
