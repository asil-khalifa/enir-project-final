        on_release: app.manager.special_push('Quiz5Wrong1')

    Button:
        text: 'Hardness'
        size_hint: 0.8, 0.7
        pos_hint: {'center_x':0.5}
        background_normal: ''
        background_color: 48/255, 194/255, 141/255, 1
        text_size: root.width/1.5, None
        on_release: app.manager.special_push('Quiz5Correct1')

    Button:
        text: 'Temperature'
        size_hint: 0.8, 0.7
